# Backend Environment Variables Template
# Copy this to .env and update with your actual values

# Application Settings
ENVIRONMENT=development
SECRET_KEY=your-secret-key-change-in-production

# Google Cloud Settings
GOOGLE_CLOUD_PROJECT_ID=df-bus-case-generator
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account-key.json

# Firebase Settings
FIREBASE_PROJECT_ID=df-bus-case-generator
FIREBASE_API_KEY=your-firebase-api-key

# VertexAI Settings
VERTEX_AI_LOCATION=us-central1
VERTEX_AI_MODEL_NAME=text-bison

# Database Settings (if using additional databases)
# DATABASE_URL=postgresql://user:password@localhost/dbname

# Redis Settings (if using Redis for caching/sessions)
# REDIS_URL=redis://localhost:6379

# Email Settings (if sending notifications)
# SMTP_HOST=smtp.gmail.com
# SMTP_PORT=587
# SMTP_USERNAME=your-email@gmail.com
# SMTP_PASSWORD=your-app-password

# Logging
LOG_LEVEL=INFO 